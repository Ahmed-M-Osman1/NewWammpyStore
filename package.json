{
  "name": "Wamppy-StoreFront",
  "version": "1.0.0",
  "description": "Wamppy-StoreFront app using nodejs, TS, eslint and jasmine. This work is part of FWD nanodegree.",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "npx tsc",
    "dev": "nodemon src/index.ts",
    "lint": "eslint src/*.ts",
    "lint:f": "eslint src/*.ts --fix",
    "jasmine": "jasmine",
    "testWithoutDatabase": "npm run build && npm run jasmine",
    "test": "npm run build && set ENV=test&& db-migrate --env test reset:test&& db-migrate --env test up:test&& npm run jasmine",
    "prettier": "prettier --write '**/*.ts'"
  },
  "author": "Ahmed M.Osman",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.18",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "supertest": "^6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0"
  }
}
